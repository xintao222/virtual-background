{
  "name": "virtual-background",
  "version": "0.1.0",
  "description": "Demo on adding virtual background to a live video stream in the browser",
  "homepage": "https://volcomix.github.io/virtual-background",
  "repository": "https://github.com/Volcomix/virtual-background.git",
  "author": "Volcomix <Volcomix@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@tensorflow-models/body-pix": "^2.0.5",
    "@tensorflow/tfjs": "^2.8.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/emscripten": "^1.39.4",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "clsx": "^1.1.1",
    "gh-pages": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "init:tflite": "yarn init:tflite:image && yarn init:tflite:container",
    "init:tflite:image": "docker build -t lib/tflite tflite",
    "init:tflite:container": "docker run -dit -v $PWD/tflite:/tflite_src -v $PWD/public/tflite:/tflite_build --name lib/tflite tflite bash",
    "start": "react-scripts start",
    "start:tflite:container": "docker start lib/tflite && yarn update:tflite:container",
    "update:tflite:container": "yarn update:tensorflow && yarn update:mediapipe",
    "update:tensorflow": "docker exec -w /tensorflow_src lib/tflite git pull",
    "update:mediapipe": "docker exec -w /mediapipe_src lib/tflite git pull",
    "build": "react-scripts build",
    "build:tflite:all": "yarn build:tflite && yarn build:tflite:simd",
    "build:tflite": "yarn build:tflite:archive && yarn extract:tflite",
    "build:tflite:archive": "docker exec -w /tflite_src lib/tflite bazel build --config=wasm -c opt :tflite",
    "build:tflite:simd": "yarn build:tflite:simd:archive && yarn extract:tflite:simd",
    "build:tflite:simd:archive": "docker exec -w /tflite_src lib/tflite bazel build --config=wasm -c opt --copt='-msimd128' :tflite-simd",
    "extract:tflite": "docker exec lib/tflite tar xvf /tflite_src/bazel-bin/tflite -C /tflite_build",
    "extract:tflite:simd": "docker exec lib/tflite tar xvf /tflite_src/bazel-bin/tflite-simd -C /tflite_build",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
